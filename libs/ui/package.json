{
  "name": "@v-md/ui",
  "type": "module",
  "version": "0.0.0",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.js"
    },
    "./dist/*": "./dist/*"
  },
  "module": "dist/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "CHANGELOG.md",
    "dist"
  ],
  "scripts": {
    "tsc": "tsc --noEmit --skipLibCheck -p tsconfig.json",
    "build-check": "cross-env BUILD_CHECK=true vite build",
    "build": "vite build"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "peerDependencies": {
    "react": "catalog:peer",
    "react-dom": "catalog:peer"
  },
  "dependencies": {
    "@v-md/core": "workspace:^",
    "@v-md/shared": "workspace:^"
  },
  "devDependencies": {
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "sass": "catalog:"
  }
}
